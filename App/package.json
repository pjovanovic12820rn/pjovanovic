{
  "name": "app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build-env": "node src/scripts/generate-env-local.ts",
    "prebuild": "node src/scripts/generate-env.ts",
    "build:prod": "npm run build-env && npm run prebuild && ng build --configuration=production",
    "build:dev": "ng build --configuration=development",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:report": "rimraf cypress/reports && npm run cy:run",
    "mocha:json": "npx mochawesome-merge \"cypress/reports/*.json\" -o cypress/reports/report.json",
    "mocha:html": "npx marge cypress/reports/report.json --reportDir cypress/reports/html --reportFilename report --inline",
    "mocha:report": "npm run mocha:json && npm run mocha:html"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0",
    "jwt-decode": "^4.0.0",
    "remixicon": "^4.6.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.2",
    "@angular/cli": "^19.0.2",
    "@angular/compiler-cli": "^19.0.0",
    "@types/jasmine": "~5.1.0",
    "cypress": "^14.2.1",
    "dotenv": "^16.5.0",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "mocha-junit-reporter": "^2.2.1",
    "mocha-multi-reporters": "^1.5.1",
    "mochawesome": "^7.1.3",
    "mochawesome-report-generator": "^6.2.0",
    "rimraf": "^6.0.1",
    "typescript": "~5.6.2"
  }
}
